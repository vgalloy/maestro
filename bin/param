#!/bin/bash

source ../conf/config

MAESTRO_TARGET=`echo "../conf/"$MAESTRO"/target/deploy"`
TOMCAT_MAESTRO_APPS=`echo "/opt/"$TOMCAT_MAESTRO$APPS_ROOT`
NORMAL="\\033[0;39m"
RED="\\033[0;31m"
BLUE="\\033[0;34m"
GREEN="\\033[1;32m"
YELLOW="\\033[1;33m"
ORANGE="\\033[0;33m"

INFO=`echo "["$BLUE "INFO" $NORMAL"]"`
WARN=`echo "["$ORANGE "WARN" $NORMAL"]"`
ERROR=`echo "["$RED "ERROR" $NORMAL"]"`
